<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.fmoon.fxapp.prototype.search.AbstractSearchActivity">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <TextField />
          <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#onSearch" text="Search" />
        </items>
      </ToolBar>
   </top>
   <center>
      <ScrollPane fitToWidth="true" prefHeight="274.0" prefWidth="534.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="listView" alignment="TOP_CENTER" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
